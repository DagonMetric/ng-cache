{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "modules",
  "projects": {
    "ng-caching": {
      "root": "modules/ng-caching",
      "projectType": "library",
      "architect": {
        "build": {
          "builder": "@dagonmetric/angular-build:lib",
          "options": {
            "extends": "angular-build.json:lib:ng-caching"
          },
          "configurations": {
            "production": {
              "environment": {
                "prod": true
              }
            }
          }
        }
      }
    }
  },
  "defaultProject": "ng-caching"
}
